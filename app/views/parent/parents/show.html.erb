<div class='container mt-5'>
  <div class='row'>
    <h3 class="page-name">
      <strong>マイページ</strong>
    </h3>
  </div>
  <div class='row'>
    <div class="d-flex p-2 bd-highlight">
      <strong>登録情報</strong>
      <div class="col">
        <%= link_to "編集する", information_edit_parent_parents_path, class: 'btn btn-outline-success' %>
      </div>
    </div>
  </div>

  <div class='row'>
    <div class="col-md-7">
      <table class="table table-bordered border-dark">
        <tr>
          <td bgcolor= "#F2F2F2", scope="col" style="width: 20%">id</td>
          <td><%= @parent.id %></td>
        </tr>
        <tr>
          <td bgcolor= "#F2F2F2", scope="col" style="width: 30%">メールアドレス</td>
          <td><%= @parent.email %></td>
        </tr>
        <tr>
          <td bgcolor= "#F2F2F2", scope="col" style="width: 20%">ログインコード</td>
          <td><%= @parent.code %></td>
        </tr>
      </table>
    </div>
  </div>
</div>